{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    "baseUrl": "./",
    "module": "ESNext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
